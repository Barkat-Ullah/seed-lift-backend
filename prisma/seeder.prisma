model Seeder {
    id          String   @id @default(auto()) @map("_id") @db.ObjectId
    fullName    String
    description String?
    email       String   @unique
    profile     String?
    phoneNumber String?
    skill       String[]

    user User @relation(fields: [email], references: [email])

    challenge Challenge[]
    createdAt DateTime    @default(now())
    updatedAt DateTime    @updatedAt

    @@map("seeder")
}

enum OrgTypeEnum {
    Startup
    Established
    NGO
}
