model Banner {
    id    String  @id @default(auto()) @map("_id") @db.ObjectId
    title String?
    image String
    type BannerType

    adminId   String?   @db.ObjectId
    admin     Admin?    @relation(fields: [adminId], references: [id])
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("banner")
}

enum BannerType {
    Founder
    Seeder
}